{
  "name": "@bipboys/webpack-polyfill-plugin",
  "version": "0.1.4",
  "description": "Node polyfills for webpack",
  "keywords": [
    "webpack",
    "webpack-plugin",
    "node",
    "typescript",
    "pligin",
    "polyfill"
  ],
  "homepage": "https://github.com/bipboys/webpack-polyfill-plugin#readme",
  "bugs": {
    "url": "https://github.com/bipboys/webpack-polyfill-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bipboys/webpack-polyfill-plugin.git"
  },
  "license": "MIT",
  "author": "Bipboys Dev Team",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/*",
    "!*.tsbuildinfo",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "tsc --p tsconfig.json"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "browserify-fs": "^1.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "console-browserify": "^1.2.0",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "domain-browser": "^4.22.0",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "punycode": "^2.1.1",
    "querystring-es3": "^0.2.1",
    "readable-stream": "^4.2.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "timers-browserify": "^2.0.12",
    "tty-browserify": "^0.0.1",
    "url": "^0.11.0",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2"
  },
  "devDependencies": {
    "webpack": "^5.74.0"
  },
  "peerDependencies": {
    "webpack": ">=5.74.0"
  }
}
